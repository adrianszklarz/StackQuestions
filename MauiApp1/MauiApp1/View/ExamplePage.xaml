<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.View.ExamplePage"
             Title="ExamplePage">
    <VerticalStackLayout>
        <Label 
            Text="Example page, that works ok"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button Command="{Binding NavigateToPage1Command}" Text="NavigateToPage1"/>
        <Label 
            Text="Counter from page1"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Label 
            Text="{Binding Counter}"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
    </VerticalStackLayout>
</ContentPage>